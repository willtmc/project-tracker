{
  "name": "electron-project-tracker",
  "version": "1.0.0",
  "description": "An Electron application for local project management",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development electron .",
    "build": "electron-builder",
    "publish": "electron-builder --publish always",
    "generate-icons": "node build/create-icons.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build:css": "tailwindcss -i ./src/renderer/tailwind.css -o ./src/renderer/styles.css --watch",
    "dev:tailwind": "concurrently \"npm run build:css\" \"npm run dev\"",
    "start:tailwind": "concurrently \"npm run build:css\" \"npm run start\"",
    "build:ts": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:prod": "eslint . -c .eslintrc-prod.js",
    "lint:prod:fix": "eslint . -c .eslintrc-prod.js --fix",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,json,md,ts,tsx}\""
  },
  "keywords": [
    "project",
    "management",
    "electron",
    "tracker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "dotenv": "^16.4.7",
    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "electron-log": "^5.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.1.7",
    "inter-ui": "^3.19.3",
    "marked": "^15.0.7",
    "openai": "^4.86.2",
    "sequelize": "^6.37.6",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.5"
  },
  "build": {
    "appId": "com.willmclemore.projecttracker",
    "productName": "Project Tracker",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "build/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "darkModeSupport": true
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "willmclemore",
        "repo": "project-tracker",
        "private": false,
        "releaseType": "release"
      }
    ]
  },
  "packageManager": "yarn@4.0.0+sha512.955259c0370ab8c06d013faa7d5e7addb6914251029695675f54e04c917ea6b092c379ba8d3521556d90b3874e5037759467072f01d2295341ead43cc259f14b"
}
